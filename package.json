{
  "name": "hsp-fo-cms",
  "version": "1.8.3",
  "description": "hsp-fo-cms",
  "files": [
    "dist/",
    "declaration/",
    "types.ts"
  ],
  "scripts": {
    "clean": "shx rm -rf ./node_modules ./dist ./test-reports ./target ./declaration *.tgz",
    "fix:prettier": "prettier --write \"./{src,test,server}/**/*.{css,md,js,jsx,json,ts,tsx}\"",
    "lint": "eslint './{src,test}/**/*'",
    "test": "jest --clearCache && jest",
    "test:light": "jest --clearCache && jest --coverage=false",
    "build:dev": "webpack --mode=development --watch",
    "build": "webpack --mode=production && tsc --project tsconfig.declaration.json",
    "start": "shx ln -sf ../index.html dist/index.html && node server/start.js",
    "deb": "echo not in use",
    "version": "echo $npm_package_version | tr -d '\n'",
    "artefactid": "echo $npm_package_name | tr -d '\n'",
    "prepare": "husky install",
    "commit": "git-cz"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://code.dev.sbb.berlin/HSP/hsp-fo-cms.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.4.1",
    "@commitlint/config-conventional": "^17.4.0",
    "@fontsource/roboto": "^4.5.8",
    "@material-ui/core": "^4.11.3",
    "@testing-library/jest-dom": "^5.12.0",
    "@testing-library/react": "^11.2.5",
    "@types/css-mediaquery": "^0.1.0",
    "@types/express": "^4.17.11",
    "@types/http-proxy": "^1.17.6",
    "@types/jest": "^27.5.2",
    "@types/node-fetch": "^2.5.10",
    "@types/react": "^17.0.3",
    "@types/react-dom": "^17.0.2",
    "@types/react-redux": "^7.1.16",
    "@types/url-join": "^4.0.0",
    "@types/wpapi": "^1.1.0",
    "@typescript-eslint/eslint-plugin": "^4.17.0",
    "@typescript-eslint/parser": "^4.17.0",
    "clean-webpack-plugin": "^3.0.0",
    "commitizen": "^4.2.6",
    "copy-webpack-plugin": "^8.1.0",
    "css-loader": "^6.7.1",
    "css-mediaquery": "^0.1.2",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^7.21.0",
    "eslint-config-prettier": "^8.6.0",
    "eslint-config-standard": "^16.0.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-promise": "^4.3.1",
    "eslint-plugin-react": "^7.22.0",
    "express": "^4.17.1",
    "hsp-web-module": "1.30.0",
    "http-proxy": "^1.18.1",
    "husky": "^8.0.0",
    "jest": "^27.2.5",
    "jest-junit": "^13.0.0",
    "msw": "^0.36.8",
    "node-fetch": "^2.6.1",
    "prettier": "^2.8.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-query": "^3.26.0",
    "react-redux": "^7.2.4",
    "react-shadow": "^19.0.2",
    "redux": "^4.1.0",
    "redux-devtools-extension": "^2.13.9",
    "shx": "^0.3.3",
    "style-loader": "^3.3.1",
    "ts-jest": "^27.1.4",
